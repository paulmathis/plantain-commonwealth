{
  "extends": ["airbnb", "plugin:jest/recommended", "prettier"],
  "rules": {
    "linebreak-style": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "react/jsx-one-expression-per-line": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  },
  "env": {
    "browser": true
  }
}
